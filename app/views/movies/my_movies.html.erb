<p id="notice"><%= notice %></p>

<h1>Listing Movies</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_movies.each do |movie| %>
      <tr>
        <td><%= movie.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

